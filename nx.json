{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultProject": "blog",
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["production", "^production"],
      "cache": true
    },
    "e2e": {
      "inputs": ["default", "^production"],
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "test": {
      "cache": true
    }
  },
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "production": ["default"],
    "sharedGlobals": []
  },
  "generators": {
    "@nx/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "none",
      "e2eTestRunner": "cypress"
    },
    "@nx/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "none"
    },
    "@nx/angular:component": {
      "style": "scss",
      "standalone": true,
      "inlineStyle": true,
      "inlineTemplate": true,
      "skipTests": true,
      "flat": true
    }
  },
  "nxCloudAccessToken": "MDUxMjQ4ZTItN2U2MS00ZjlmLWE0OWQtM2ZiM2RlMDgwMTIyfHJlYWQtd3JpdGU="
}
