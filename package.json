{
  "name": "@senior-robot/source",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "pnpm run svg && nx serve blog",
    "start:host": "pnpm run svg && nx serve blog --host",
    "start:prod": "pnpm run svg && pnpm nx serve blog --configuration=production",
    "build": "pnpm run svg && pnpm nx build blog",
    "svg": "svg-generator"
  },
  "svgGenerator": {
    "outputPath": "./blog/src/app/svg/",
    "srcPath": "./blog/src/public/assets/",
    "svgoConfig": {
      "plugins": [
        "removeDimensions",
        "cleanupAttrs",
        {
          "name": "convertColors",
          "params": {
            "currentColor": true
          }
        }
      ]
    }
  },
  "private": false,
  "dependencies": {
    "@analogjs/content": "1.19.4",
    "@analogjs/router": "1.19.4",
    "@angular/animations": "20.2.4",
    "@angular/common": "20.2.4",
    "@angular/compiler": "20.2.4",
    "@angular/core": "20.2.4",
    "@angular/forms": "20.2.4",
    "@angular/platform-browser": "20.2.4",
    "@angular/platform-browser-dynamic": "20.2.4",
    "@angular/platform-server": "20.2.4",
    "@angular/router": "20.2.4",
    "@angular/ssr": "20.2.2",
    "@jsverse/transloco": "^7.4.2",
    "@ngneat/overview": "^6.1.1",
    "@ngneat/svg-icon": "^7.1.6",
    "@ngxpert/hot-toast": "^4.1.2",
    "@tanstack/angular-form": "1.1.2",
    "dotenv": "^16.4.7",
    "front-matter": "^4.0.2",
    "luxon": "^3.4.0",
    "mailtrap": "^3.3.0",
    "marked": "^15.0.7",
    "marked-gfm-heading-id": "^4.1.1",
    "marked-highlight": "^2.2.1",
    "marked-mangle": "^1.1.10",
    "prismjs": "^1.29.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@analogjs/platform": "1.19.4",
    "@analogjs/vite-plugin-angular": "1.19.4",
    "@angular-devkit/build-angular": "20.2.2",
    "@angular-devkit/core": "20.2.2",
    "@angular-devkit/schematics": "20.2.2",
    "@angular-eslint/eslint-plugin": "19.0.2",
    "@angular-eslint/eslint-plugin-template": "19.0.2",
    "@angular-eslint/template-parser": "19.0.2",
    "@angular/cli": "~20.2.0",
    "@angular/compiler-cli": "20.2.4",
    "@angular/language-service": "20.2.4",
    "@ngneat/svg-generator": "^7.0.2",
    "@nx/angular": "21.5.1",
    "@nx/cypress": "21.5.1",
    "@nx/devkit": "21.5.1",
    "@nx/eslint": "21.5.1",
    "@nx/eslint-plugin": "21.5.1",
    "@nx/js": "21.5.1",
    "@nx/vite": "21.5.1",
    "@nx/workspace": "21.5.1",
    "@schematics/angular": "20.2.2",
    "@tailwindcss/typography": "^0.5.10",
    "@types/luxon": "^3.3.4",
    "@types/node": "18.19.15",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@typescript-eslint/utils": "7.18.0",
    "@vercel/node": "^2.15.9",
    "autoprefixer": "^10.4.14",
    "cypress": "14.4.1",
    "daisyui": "^3.5.1",
    "destr": "^2.0.1",
    "jiti": "2.4.2",
    "jsdom": "^22.1.0",
    "nx": "21.5.1",
    "postcss": "^8.4.27",
    "postcss-nesting": "^12.0.1",
    "prettier": "^3.0.1",
    "prettier-plugin-tailwindcss": "^0.5.2",
    "tailwindcss": "^3.3.3",
    "typescript": "5.9.2",
    "vite": "7.1.5",
    "vitest": "1.4.0"
  }
}
