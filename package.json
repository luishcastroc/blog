{
  "name": "@senior-robot/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run svg && nx serve blog",
    "start:prod": "npm run svg && nx serve blog --configuration=production",
    "build": "npm run svg && nx build blog",
    "svg": "svg-generator"
  },
  "svgGenerator": {
    "outputPath": "./blog/src/app/svg/",
    "srcPath": "./blog/src/public/assets/",
    "svgoConfig": {
      "plugins": [
        "removeDimensions",
        "cleanupAttrs",
        {
          "name": "convertColors",
          "params": {
            "currentColor": true
          }
        }
      ]
    }
  },
  "private": false,
  "dependencies": {
    "@analogjs/content": "^0.2.0-beta.25",
    "@analogjs/router": "^0.2.0-beta.25",
    "@angular/animations": "~16.1.0",
    "@angular/common": "~16.1.0",
    "@angular/compiler": "~16.1.0",
    "@angular/core": "~16.1.0",
    "@angular/forms": "~16.1.0",
    "@angular/platform-browser": "~16.1.0",
    "@angular/platform-browser-dynamic": "~16.1.0",
    "@angular/platform-server": "~16.1.0",
    "@angular/router": "~16.1.0",
    "@ngneat/svg-icon": "^7.0.0",
    "front-matter": "^4.0.2",
    "luxon": "^3.3.0",
    "marked": "^5.0.2",
    "marked-gfm-heading-id": "^3.0.4",
    "marked-highlight": "^2.0.1",
    "prismjs": "^1.29.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@analogjs/platform": "^0.2.0-beta.25",
    "@analogjs/vite-plugin-angular": "^0.2.0-beta.25",
    "@angular-devkit/build-angular": "~16.1.0",
    "@angular-devkit/core": "~16.1.0",
    "@angular-devkit/schematics": "~16.1.0",
    "@angular/cli": "~16.1.0",
    "@angular/compiler-cli": "~16.1.0",
    "@angular/language-service": "~16.1.0",
    "@ngneat/svg-generator": "^7.0.1",
    "@nx/angular": "16.6.0",
    "@nx/cypress": "16.6.0",
    "@nx/devkit": "16.6.0",
    "@nx/js": "16.6.0",
    "@nx/vite": "16.6.0",
    "@nx/workspace": "16.6.0",
    "@schematics/angular": "~16.1.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/luxon": "^3.3.1",
    "@types/node": "16.11.7",
    "@vercel/node": "^2.15.8",
    "autoprefixer": "^10.4.0",
    "cypress": "^12.16.0",
    "daisyui": "^3.2.1",
    "jsdom": "^22.0.0",
    "nx": "16.6.0",
    "nx-cloud": "16.1.1",
    "postcss": "^8.4.5",
    "postcss-nesting": "^12.0.0",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.2",
    "typescript": "~5.1.3",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.32.2"
  }
}
