{
  "name": "@senior-robot/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "pnpm run svg && nx serve blog",
    "start:host": "pnpm run svg && nx serve blog --host",
    "start:prod": "pnpm run svg && pnpm nx serve blog --configuration=production",
    "build": "pnpm run svg && pnpm nx build blog",
    "svg": "svg-generator"
  },
  "svgGenerator": {
    "outputPath": "./blog/src/app/svg/",
    "srcPath": "./blog/src/public/assets/",
    "svgoConfig": {
      "plugins": [
        "removeDimensions",
        "cleanupAttrs",
        {
          "name": "convertColors",
          "params": {
            "currentColor": true
          }
        }
      ]
    }
  },
  "private": false,
  "dependencies": {
    "@analogjs/content": "^0.2.19",
    "@analogjs/router": "^0.2.19",
    "@angular/animations": "17.0.1",
    "@angular/common": "17.0.1",
    "@angular/compiler": "17.0.1",
    "@angular/core": "17.0.1",
    "@angular/forms": "17.0.1",
    "@angular/platform-browser": "17.0.1",
    "@angular/platform-browser-dynamic": "17.0.1",
    "@angular/platform-server": "17.0.1",
    "@angular/router": "17.0.1",
    "@ngneat/hot-toast": "^6.0.1",
    "@ngneat/overview": "^5.1.1",
    "@ngneat/svg-icon": "^7.1.0",
    "@ngneat/transloco": "^6.0.0",
    "dotenv": "^16.3.1",
    "front-matter": "^4.0.2",
    "luxon": "^3.4.0",
    "mailtrap": "^3.2.0",
    "marked": "^5.1.2",
    "marked-gfm-heading-id": "^3.1.0",
    "marked-highlight": "^2.0.6",
    "prismjs": "^1.29.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@analogjs/platform": "^0.2.19",
    "@analogjs/vite-plugin-angular": "^0.2.19",
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular-devkit/core": "^17.0.0",
    "@angular-devkit/schematics": "^17.0.0",
    "@angular/cli": "~17.0.0",
    "@angular/compiler-cli": "17.0.1",
    "@angular/language-service": "17.0.1",
    "@ngneat/svg-generator": "^7.0.2",
    "@nx/angular": "17.2.8",
    "@nx/cypress": "17.2.8",
    "@nx/devkit": "17.2.8",
    "@nx/js": "17.2.8",
    "@nx/vite": "17.2.8",
    "@nx/workspace": "17.2.8",
    "@schematics/angular": "17.0.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/luxon": "^3.3.4",
    "@types/node": "20.9.0",
    "@vercel/node": "^2.15.9",
    "autoprefixer": "^10.4.14",
    "cypress": "^13.0.0",
    "daisyui": "^3.9.4",
    "destr": "^2.0.1",
    "jsdom": "^22.1.0",
    "nx": "17.2.8",
    "postcss": "^8.4.27",
    "postcss-nesting": "^12.0.1",
    "prettier": "^3.0.1",
    "prettier-plugin-tailwindcss": "^0.5.2",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.2.2",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.1"
  }
}

